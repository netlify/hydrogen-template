    <script>
        const PASSWORD_EXPIRY_TIME = 2 * 60 * 60 * 1000; // 2 hours in milliseconds

        // Check if the password has already been entered and is still valid
        let isPasswordEntered = false;
        const passwordTimestamp = sessionStorage.getItem('passwordTimestamp');
        if (passwordTimestamp && (Date.now() - parseInt(passwordTimestamp, 10)) < PASSWORD_EXPIRY_TIME) {
            isPasswordEntered = true;
        } else {
            sessionStorage.removeItem('passwordTimestamp');
        }

        function generateSignal() {
            const signals = ['Up', 'Down'];
            const randomSignal = signals[Math.floor(Math.random() * signals.length)];
            const selectedPair = document.getElementById('trading-pair-select').value;
            const signalResultElement = document.getElementById('signal-result');
            const arrow = randomSignal === 'Up' ? 'â¬†' : 'â¬‡';
            signalResultElement.innerHTML = `Signal: ${arrow} ${randomSignal} for ${selectedPair}`;
            signalResultElement.style.color = randomSignal === 'Up' ? 'green' : 'red';
            startCountdown();
        }

        function startCountdown() {
            const countdownElement = document.getElementById('countdown');
            const progressElement = document.getElementById('countdown-progress');
            const getSignalBtn = document.getElementById('get-signal-btn');
            let timeLeft = 60; // Set to 1 minute (60 seconds)

            countdownElement.innerText = `Next signal in: ${timeLeft} seconds`;
            progressElement.value = 0;
            progressElement.max = timeLeft;
            progressElement.style.display = 'block';
            getSignalBtn.style.display = 'none'; // Hide the button

            const countdownInterval = setInterval(() => {
                timeLeft--;
                countdownElement.innerText = `Next signal in: ${timeLeft} seconds`;
                progressElement.value = 60 - timeLeft;

                if (timeLeft <= 0) {
                    clearInterval(countdownInterval);
                    countdownElement.innerText = '';
                    progressElement.style.display = 'none';
                    getSignalBtn.style.display = 'block'; // Show the button again
                }
            }, 1000);
        }

