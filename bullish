<script>
function closeGlowingHeadline() {
    const banner = document.getElementById('glowing-headline');
    banner.style.transform = 'translateY(-100%)';
    setTimeout(() => banner.remove(), 300);
    localStorage.setItem('glowingHeadlineClosed', 'true');
}

window.addEventListener('load', () => {
    if (!localStorage.getItem('glowingHeadlineClosed')) {
        document.getElementById('glowing-headline').style.display = 'flex';
    }
});
</script>
    

