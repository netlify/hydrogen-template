<div class="grok" style="opacity: 0.3; font-family: Arial, sans-serif; color: rgb(254, 254, 254); font-size: 1.5em; left: 1127px; top: 418px;">Grok</div>

<style>
    @media (max-width: 768px) {
        .grok {
            font-size: 1em;
        }
    }
</style>


<script>
    // Grok Mouse Tracker
    const grok = document.querySelector('.grok');
    let posX = 0, posY = 0;
    let mouseX = 0, mouseY = 0;
    const speed = 0.2;

    document.addEventListener('mousemove', (e) => {
        mouseX = e.clientX;
        mouseY = e.clientY;
    });

    const animateGrok = () => {
        posX += (mouseX - posX) * speed;
        posY += (mouseY - posY) * speed;
        grok.style.left = `${posX}px`;
        grok.style.top = `${posY}px`;
        requestAnimationFrame(animateGrok);
    };
    animateGrok();

    // Matrix Rain Effect
    function createMatrixRain() {
        const chars = '01';
        const container = document.querySelector('.matrix-rain');
        
        function createDrop() {
            const element = document.createElement('div');
            element.style.position = 'absolute';
            element.style.left = `${Math.random() * 100}%`;
            element.style.color = `hsl(${Math.random() * 360}, 100%, 50%)`;
            element.style.animation = `fall ${Math.random() * 3 + 2}s linear`;
            element.textContent = chars[Math.floor(Math.random() * chars.length)];
            element.style.pointerEvents = 'none';
            container.appendChild(element);
            
            setTimeout(() => element.remove(), 5000);
        }
        
        setInterval(createDrop, 100);
    }
    createMatrixRain();
</script>

