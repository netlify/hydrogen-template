    <script>
        // Disable right-click menu
        document.addEventListener('contextmenu', function(e) {
            e.preventDefault();
        });
    
        // Disable keyboard shortcuts for viewing source or inspecting elements
        document.addEventListener('keydown', function(e) {
            // List of blocked keys
            if (
                // Disable F12 (DevTools)
                e.keyCode === 123 || 
                // Disable Ctrl+Shift+I, Ctrl+Shift+J, Ctrl+U for Windows
                (e.ctrlKey && e.shiftKey && ['I', 'J', 'C'].includes(e.key)) || 
                (e.ctrlKey && e.key === 'U') ||
                // Disable Command+Option+U, Command+Shift+C for Mac
                (e.metaKey && e.altKey && e.key === 'u') ||
                (e.metaKey && e.shiftKey && e.key === 'c') ||
                (e.metaKey && e.key === 'u') ||
                // Additional checks for other combinations
                (e.ctrlKey && e.shiftKey && e.key === 'K')
            ) {
                e.preventDefault();
                alert('This action is disabled.');
            }
        });
    
        // Extra: Disable drag-and-drop text or image selection
        document.addEventListener('dragstart', function(e) {
            e.preventDefault();
        });
    </script>
